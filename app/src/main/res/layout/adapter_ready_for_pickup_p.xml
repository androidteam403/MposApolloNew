        <LinearLayout
            android:id="@+id/fullfillment_parent_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="visible">

            <LinearLayout
                android:id="@+id/order_child_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/square_stroke_bg"
                android:orientation="horizontal"
                android:padding="@dimen/dimen2dp"
                android:weightSum="1">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight=".5"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/fullfillment_select_icon"
                        android:layout_width="14dp"
                        android:layout_height="14dp"
                        android:layout_marginStart="@dimen/dimen2dp"
                        android:clickable="false"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/rounded_background"
                        android:tint="@color/green"/>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dimen10dp"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/filment_id"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/roboto_regular"
                                android:text="FL2920199228888399292"
                                android:textColor="@color/black"
                                android:textSize="@dimen/dimen10sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/roboto_medium"
                                android:text="Stock Availability : "
                                android:textColor="#b6b4af"
                                android:textSize="@dimen/dimen10sp" />

                            <TextView
                                android:id="@+id/pickup_status"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/roboto_regular"
                                android:textColor="@color/black"
                                android:textSize="@dimen/dimen10sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/roboto_medium"
                                android:text="Source ID : "
                                android:textColor="#b6b4af"
                                android:textSize="@dimen/dimen10sp" />

                            <TextView
                                android:id="@+id/source_id"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/roboto_regular"
                                android:textColor="@color/black"
                                android:textSize="@dimen/dimen10sp" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight=".3"
                    android:gravity="end"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/three_dp"
                        android:gravity="end"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_medium"
                            android:text="Delivery Date"
                            android:textColor="#b6b4af"
                            android:textSize="@dimen/nine_sp" />

                        <TextView
                            android:id="@+id/delivery_date_pickpack"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/one_dp"
                            android:fontFamily="@font/roboto_regular"
                            android:gravity="center"
                            android:text="2023/01/18 - 21:00:00"
                            android:textColor="@color/black"
                            android:textSize="@dimen/nine_sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical|end"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="items"
                            android:textColor="#b6b4af"
                            android:textSize="@dimen/dimen_9_sp" />

                        <TextView
                            android:id="@+id/total_items"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dimen2dp"
                            android:fontFamily="@font/roboto_bold"
                            android:text="45"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimen14sp" />

                        <ImageView
                            android:id="@+id/right_arrow"
                            android:layout_width="@dimen/dimen25dp"
                            android:layout_height="@dimen/dimen25dp"
                            android:layout_gravity="end"
                            android:contentDescription="@string/app_name"
                            android:src="@drawable/right_arrow_black"
                            android:visibility="visible" />
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/down_arrow"
                        android:layout_width="30dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="130dp"
                        android:contentDescription="@string/app_name"
                        android:rotation="90"
                        android:src="@drawable/black_right_arroww"
                        android:visibility="gone" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/tag_box"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight=".2"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:visibility="visible">

                    <ImageView
                        android:layout_width="@dimen/dimen22dp"
                        android:layout_height="@dimen/dimen22dp"
                        android:contentDescription="@string/app_name"
                        android:tint="#00aeef"
                        android:src="@drawable/tag_box" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Scan"
                        android:textColor="@color/black"
                        android:textSize="@dimen/dimen_11sp"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/scan_delete_layout"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight=".2"
                    android:gravity="center_vertical|end"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center_horizontal|center_vertical"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:contentDescription="@string/app_name"
                            android:src="@drawable/cart"
                            android:tint="#757575" />

                        <TextView
                            android:id="@+id/scannedCode"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="001"
                            android:textColor="#757575"
                            android:textSize="8sp"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/scan_delete"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:src="@drawable/ic_not_available"
                        android:layout_marginStart="@dimen/dimen2dp"
                        android:layout_marginEnd="@dimen/dimen8dp"/>
                </LinearLayout>
            </LinearLayout>

            <!--<TextView
                android:id="@+id/print_qr_code"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:background="#00aeef"
